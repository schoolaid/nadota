{
  "name": "schoolaid/nadota",
  "version": "1.0.7",
  "type": "library",
  "description": "Laravel admin panel package for resource-based CRUD interfaces similar to Laravel Nova",
  "keywords": [
    "laravel",
    "admin",
    "crud",
    "resource",
    "panel",
    "nova",
    "schoolaid"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "SchoolAid Development Team",
      "email": "dev@schoolaid.com"
    }
  ],
  "require": {
    "php": "^8.2|^8.3",
    "laravel/framework": "^11.0|^12.0",
    "inertiajs/inertia-laravel": "^2.0",
    "phpoffice/phpspreadsheet": "^1.30|^2.0|^3.0"
  },
  "require-dev": {
    "pestphp/pest": "^3.0",
    "pestphp/pest-plugin-laravel": "^3.0",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0",
    "fakerphp/faker": "^1.23",
    "laravel/sanctum": "^4.0",
    "orchestra/testbench": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "SchoolAid\\Nadota\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SchoolAid\\Nadota\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SchoolAid\\Nadota\\NadotaServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "pest",
    "test-coverage": "pest --coverage",
    "analyse": "phpstan analyse",
    "format": "pint"
  }
}